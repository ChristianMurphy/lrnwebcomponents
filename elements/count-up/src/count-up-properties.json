{
  /**
   * Starting point for counting
   */
  "start": {
    "name": "start",
    "type": "Number",
    "value": 0
  },
  /**
   * End point for counting stopping
   */
  "end": {
    "name": "end",
    "type": "Number",
    "value": 100
  },
  /**
   * Duration to count
   */
  "duration": {
    "name": "duration",
    "type": "Number",
    "value": 2.5
  },
  /**
   * Disable easing animation
   */
  "noeasing": {
    "name": "noeasing",
    "type": "Boolean",
    "value": false
  },
  /**
   * decimal places to show
   */
  "decimalplaces": {
    "name": "decimalPlaces",
    "type": "Number",
    "value": 0
  },
  /**
   * separator for 100s groupings
   */
  "separator": {
    "name": "separator",
    "type": "String",
    "value": ","
  },
  /**
   * decimal point character
   */
  "decimal": {
    "name": "decimal",
    "type": "String",
    "value": "."
  },
  /**
   * prefix string before the number counting
   */
  "prefixtext": {
    "name": "prefixtext",
    "type": "String",
    "value": " "
  },
  /**
   * suffix string after the number counting
   */
  "suffixtext": {
    "name": "suffixtext",
    "type": "String",
    "value": " "
  },
  "thresholds": {
    "type": "Array",
    "value": [0.0, 0.25, 0.5, 0.75, 1.0]
  },
  "rootMargin": {
    "type": "String",
    "value": "0px"
  },
  "ratio": {
    "type": "Number",
    "reflectToAttribute": true,
    "readOnly": true
  },
  "visibleLimit": {
    "type": "Number",
    "value": 0.5,
    "reflectToAttribute": true
  }
}
