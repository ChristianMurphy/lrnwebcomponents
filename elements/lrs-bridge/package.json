{
  "name": "@lrnwebcomponents/lrs-bridge",
  "wcfactory": {
    "className": "LrsBridge",
    "customElementClass": "PolymerElement",
    "elementName": "lrs-bridge",
    "generator-wcfactory-version": "0.7.0",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/lrs-bridge.css",
      "scss": "src/lrs-bridge.scss",
      "html": "src/lrs-bridge.html",
      "js": "src/lrs-bridge.js",
      "properties": "src/lrs-bridge-properties.json",
      "hax": "src/lrs-bridge-hax.json"
    }
  },
  "version": "0.0.0",
  "description": "LRS element that captures lrn-emitter events and forwards them to the learnig record store.",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "lrs-bridge.js",
  "module": "lrs-bridge.es6.js",
  "umd": "lrs-bridge.umd.js",
  "amd": "lrs-bridge.amd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/lrs-bridge/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "open ./src && concurrently --kill-others 'yarn run watch' 'yarn run serve'",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "heyMP"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@polymer/polymer": "^3.0.5",
    "apollo-boost": "^0.3.1",
    "graphql-tag": "^2.10.1"
  },
  "devDependencies": {
    "@lrnwebcomponents/lrs-emitter": "^0.0.0",
    "@polymer/iron-demo-helpers": "3.0.2",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@webcomponents/webcomponentsjs": "2.1.3",
    "concurrently": "4.0.1",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.4",
    "lodash": "4.17.11",
    "polymer-cli": "1.8.1",
    "polymer-build": "3.1.0",
    "wct-browser-legacy": "1.0.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}