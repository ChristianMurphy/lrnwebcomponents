{
  "name": "@lrnwebcomponents/lrndesign-timeline",
  "wcfactory": {
    "className": "LrndesignTimeline",
    "customElementClass": "LitElement",
    "elementName": "lrndesign-timeline",
    "generator-wcfactory-version": "0.7.1",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/lrndesign-timeline.css",
      "scss": "src/lrndesign-timeline.scss",
      "html": "src/lrndesign-timeline.html",
      "js": "src/lrndesign-timeline.js",
      "properties": "src/lrndesign-timeline-properties.json",
      "hax": "src/lrndesign-timeline-hax.json"
    },
    "sharedStyles": [
      "...super.styles"
    ],
    "sharedProperties": [
      "...super.properties"
    ]
  },
  "version": "2.7.7",
  "description": "an element that displays events on a timeline",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "lrndesign-timeline.js",
  "module": "lrndesign-timeline.js",
  "umd": "lrndesign-timeline.umd.js",
  "scripts": {
    "test": "wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/lrndesign-timeline/test/",
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && rollup -c && prettier --ignore-path ../../.prettierignore --write \"**/*.{js,json}\"",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs"
  },
  "author": {
    "name": "nikkimk"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/responsive-utility": "^2.7.7",
    "@lrnwebcomponents/schema-behaviors": "^2.7.7",
    "@lrnwebcomponents/simple-colors": "^2.7.7",
    "lit-element": "^2.3.1"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^2.7.7",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@webcomponents/webcomponentsjs": "2.2.7",
    "concurrently": "4.1.0",
    "gulp-babel": "8.0.0",
    "lodash": "4.17.19",
    "polymer-build": "3.1.2",
    "polymer-cli": "1.9.7",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.1"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
